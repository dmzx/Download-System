{
	"name": "dmzx/downloadsystem",
	"type": "phpbb-extension",
	"description": "This extension will add an easy download system to your forum, where you can offer your user to download files from your webspace.",
	"homepage": "https://www.dmzx-web.net",
	"version": "1.1.0",
	"time": "2019-05-08",
	"keywords": [
		"phpbb",
		"extension",
		"downloadsystem"
	],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		},
		{
			"name": "Die Muellers Dot Org",
			"homepage": "http://die-muellers.org",
			"role": "Original MOD author"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master",
		"phpunit/phpunit": "^4.8.35"
	},
	"extra": {
		"display-name": "Download System",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0,<3.3.0@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "downloadsystem_version.json",
			"ssl": true
		}
	}
}
